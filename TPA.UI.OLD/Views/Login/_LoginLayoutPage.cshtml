<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/assets/css/style.bundle.css")
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    @Styles.Render("~/Content/assets/vendors/custom/fullcalendar/fullcalendar.bundle.css")

    <!--end::Page Vendors Styles -->
    <!--begin:: Global Mandatory Vendors -->
    @Styles.Render("~/Content/assets/vendors/general/perfect-scrollbar/css/perfect-scrollbar.css")

    <!--end:: Global Mandatory Vendors -->
    <!--begin:: Global Optional Vendors -->
    @Styles.Render("~/Content/assets/vendors/general/tether/dist/css/tether.css")
    @Styles.Render("~/Content/assets/vendors/general/bootstrap-datepicker/dist/css/bootstrap-datepicker3.css")
    @Styles.Render("~/Content/assets/vendors/general/bootstrap-datetime-picker/css/bootstrap-datetimepicker.css")
    @Styles.Render("~/Content/assets/vendors/general/bootstrap-timepicker/css/bootstrap-timepicker.css")
    @Styles.Render("~/Content/assets/vendors/general/bootstrap-daterangepicker/daterangepicker.css")
    @Styles.Render("~/Content/assets/vendors/general/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.css")
    @Styles.Render("~/Content/assets/vendors/general/bootstrap-select/dist/css/bootstrap-select.css")
    @Styles.Render("~/Content/assets/vendors/general/select2/dist/css/select2.css")
    @Styles.Render("~/Content/assets/vendors/general/nouislider/distribute/nouislider.css")
    @Styles.Render("~/Content/assets/vendors/general/owl.carousel/dist/assets/owl.carousel.css")
    @Styles.Render("~/Content/assets/vendors/general/owl.carousel/dist/assets/owl.theme.default.css")
    @Styles.Render("~/Content/assets/vendors/general/dropzone/dist/dropzone.css")
    @Styles.Render("~/Content/assets/vendors/general/bootstrap-markdown/css/bootstrap-markdown.min.css")
    @Styles.Render("~/Content/assets/vendors/general/animate.css/animate.css")
    @Styles.Render("~/Content/assets/vendors/general/toastr/build/toastr.css")
    @Styles.Render("~/Content/assets/vendors/general/morris.js/morris.css")
    @Styles.Render("~/Content/assets/vendors/general/sweetalert2/dist/sweetalert2.css")
    @Styles.Render("~/Content/assets/vendors/general/socicon/css/socicon.css")
    @Styles.Render("~/Content/assets/vendors/custom/vendors/line-awesome/css/line-awesome.css")
    @Styles.Render("~/Content/assets/vendors/custom/vendors/flaticon/flaticon.css")
    @Styles.Render("~/Content/assets/vendors/custom/vendors/flaticon2/flaticon.css")
    @Styles.Render("~/Content/assets/vendors/general/fortawesome/fontawesome-free/css/all.min.css")

    <!--end:: Global Optional Vendors -->
    <!--begin::Global Theme Styles(used by all pages) -->
    @Styles.Render("~/Content/assets/css/style.bundle.css")

    <!--end::Global Theme Styles -->
    <!--begin::Layout Skins(used by all pages) -->
    @Styles.Render("~/Content/assets/css/skins/header/base/dark.css")
    @Styles.Render("~/Content/assets/css/skins/header/menu/dark.css")
    @Styles.Render("~/Content/assets/css/skins/brand/dark.css")
    @Styles.Render("~/Content/assets/css/skins/aside/dark.css")
    @Styles.Render("~/Content/login.css")
    <script>
        WebFont.load({
            google: {
                "families": ["Poppins:300,400,500,600,700", "Roboto:300,400,500,600,700"]
            },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>
</head>
<body>
    <div class="login-container">
        @RenderBody()
    </div>
</body>
</html>
